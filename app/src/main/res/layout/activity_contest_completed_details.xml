<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_contest_completed_details"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sticker_android.controller.activities.common.contestDetails.ContestCompletedDetailsActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/toolbar" />

        <include layout="@layout/contest_completed_list" />

    </LinearLayout>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fabShowAllItems"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_margin="@dimen/add_corp_margin"
        android:elevation="6dp"
        app:backgroundTint="@color/colorFloatingCorporate"
        app:srcCompat="@drawable/ic_other" />
</RelativeLayout>
